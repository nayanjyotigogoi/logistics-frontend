{
  "name": "logistics-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:3001": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "api": "npx @openapitools/openapi-generator-cli generate -g typescript-axios -i api-docs/openapi.json -o src/api --global-property skipFormModel=false",
    "pre-checks": "npm run prettier && npm run lint:fix && npm run build",
    "prepare": "husky"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@iconify/react": "^4.1.1",
    "@reduxjs/toolkit": "^2.2.1",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.6.7",
    "framer-motion": "^11.0.5",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.65.0",
    "react-redux": "^9.1.0",
    "sonner": "^2.0.7",
    "swagger-typescript-api": "^13.2.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-define-config": "^2.1.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "logistics",
    "freight",
    "nextjs",
    "typescript"
  ],
